{
    "title": "UIUX Engineering Data Schema",
    "type": "Object",
    "properties": {
      "term": {
        "type": "String",
        "description": "Term to be defined."
      },
      "definition": {
        "type": "String",
        "description": "Definition of term."
      },
      "references": {
        "type": "Array",
        "description": "Array of url titles and links.",
        "ArrayProperties": {
          "title": {
            "type": "String"
          },
          "url": {
            "type": "String"
          }
        },
        "required": [
          "title",
          "url"
        ]
      },
      "seed": {
        "type": "Object",
        "description": "Code repository.",
        "properties": {
          "title": {
            "type": "String"
          },
          "server": {
            "type": "String",
            "description": "github, bitbucket, tfs"
          },
          "type": {
            "type": "String",
            "description": "git, svn"
          },
          "url": {
            "type": "String"
          }
        },
        "required": [
          "title",
          "url"
        ]
      },
      "tags": {
        "type": "Array",
        "description": "taxonomy words or phrases"
      },
      "dataSet": {
        "type":"String",
        "description": "url to data json without the .json extension. For Example: angular2/angular2-articles"
      }
    },
    "required": [
      "term"
    ]
  }